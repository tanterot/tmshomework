CREATE TABLE orders(
    id INT PRIMARY KEY ,
    titleOrder VARCHAR,
    dataOrder DATE,
    numberOfProduct INT,
    costOrder money,
    delivery BOOLEAN);

INSERT INTO orders (id,titleOrder,dataOrder,numberOfProduct,costOrder,delivery)
values (1,'magic book x14','2024-05-17',10,'$650',true),
       (2,'magic book x16','2024-05-18',10,'$750',true);

ALTER TABLE orders RENAME COLUMN delivery TO isDelivery;

DROP TABLE orders;
